                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page content -->
    <div class="page-content">

        <!-- Main sidebar -->
        <div class="sidebar navbar-custom sidebar-main sidebar-expand-md">

            <!-- Sidebar mobile toggler -->
            <div class="sidebar-mobile-toggler text-center">
                <a href="#" class="sidebar-mobile-main-toggle">
                    <i class="icon-arrow-left8"></i>
                </a>
                Navigationss
                <a href="#" class="sidebar-mobile-expand">
                    <i class="icon-screen-full"></i>
                    <i class="icon-screen-normal"></i>
                </a>
               
            </div>
            <!-- /sidebar mobile toggler -->
            <!-- Sidebar content -->
            <div class="sidebar-content">

                <!-- User menu -->
                <div class="sidebar-user" style="display:none">
                    <div class="card-body">
                        <div class="media">
                            <div class="mr-3">
                                <a href="#">
                                   
                                    <img src="~/otherfiles/global_assets/images/demo/users/face11.jpg" width="38" height="38" class="rounded-circle" alt="">;
                                </a>
                            </div>

                            <div class="media-body">
                                <div class="media-title font-weight-semibold" id="dvUserName"></div>
                                <div class="font-size-xs opacity-50" id="dvRoleName">
                                
                                </div>
                            </div>


                        </div>
                    </div>
                </div>
                <!-- /user menu -->
                <!-- Main navigation -->
                <div class="card card-sidebar-mobile">
                    <ul class="nav nav-sidebar" data-nav-type="accordion" id="mMenu">
                        <!-- Main -->
                        <li class="nav-item-header"><div class="text-uppercase font-size-xs line-height-xs">Navigation</div> <i class="icon-menu" title="Main"></i></li>

                    </ul>
                    <a asp-controller="LoanDetail" asp-action="LoanDetail">Loan Detail</a>
                </div>
