            </div>
            <!-- /sidebar content -->

        </div>
        <!-- /main sidebar -->


        <div class="content-wrapper">

            @RenderBody()

            <!-- Footer -->
            <div class="navbar navbar-expand-lg navbar-light">
                <div class="text-center d-lg-none w-100">
                    <button type="button" class="navbar-toggler dropdown-toggle" data-toggle="collapse" data-target="#navbar-footer">
                        <i class="icon-unfold mr-2"></i>
                        Footer
                    </button>
                </div>

                <div class="navbar-collapse collapse" id="navbar-footer">
                    <span class="navbar-text">
                        &copy; @DateTime.Now.Year <a href="#"></a>  <a href="https://www.smcfinance.com/" target="_blank">Moneywise Financial Services Pvt. Ltd.</a>
                    </span>


                </div>
            </div>
            <!-- /footer -->
        </div>


    </div>
    <div id="modal_form_ChangePassword" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
        <div class="modal-dialog">
            <form class="form-validate-jquerylayout" onsubmit="return LayoutFormsubmit();" name="registration">
                <div id="Password_content">

                </div>
