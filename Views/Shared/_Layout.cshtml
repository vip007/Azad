    <script>

        $(document).ready(function () {
            $('.datefrom').datepicker({
                dateFormat: 'yy-mm-dd',
                defaultDate: new Date(),
                changeMonth: true,
                changeYear: true,
                inline: true,
                maxDate: new Date(),
                onSelect: function (dateStr) {
                    $(".dateto").val(dateStr);
                    $(".dateto").datepicker({
                        minDate: new Date(dateStr),
                        dateFormat: 'yy-mm-dd',
                        changeMonth: true,
                        changeYear: true,
                        maxDate: new Date(),
                    })
                }
            });
        });
    </script>
    @RenderSection("Scripts", required: false)

    @*$("body").addClass("loading");
        $("body").removeClass("loading");*@

